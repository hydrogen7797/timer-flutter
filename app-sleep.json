{
  "expo": {
    "name": "SLEEP 3min",
    "slug": "sleep-3min",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-sleep.png",
    "scheme": "sleep3min",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.timer.sleep3min"
    },
    "android": {
      "package": "com.timer.sleep3min",
      "adaptiveIcon": {
        "backgroundColor": "#111111",
        "foregroundImage": "./assets/images/icon-sleep.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-sleep.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111111"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "appType": "SLEEP"
    }
  }
}
