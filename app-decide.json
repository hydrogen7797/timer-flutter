{
  "expo": {
    "name": "NOW NOT DECIDE",
    "slug": "now-not-decide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-decide.png",
    "scheme": "nownotdecide",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.timer.nownotdecide"
    },
    "android": {
      "package": "com.timer.nownotdecide",
      "adaptiveIcon": {
        "backgroundColor": "#111111",
        "foregroundImage": "./assets/images/icon-decide.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-decide.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111111"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "appType": "DECIDE"
    }
  }
}
